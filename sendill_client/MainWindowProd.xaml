                    <Button.ContextMenu>
                        <ContextMenu>
                            <StackPanel>
                                <Label Content="{Binding Path=owner}" FontSize="14" Margin="4"></Label>
                                <CheckBox IsChecked="{Binding car1, Mode=OneWay}" Content="Lyfta" Margin="4"/>
                                <CheckBox IsChecked="{Binding car2, Mode=OneWay}" Content="Dregur"  Margin="4"/>
                                <CheckBox IsChecked="{Binding car3, Mode=OneWay}" Content="Krókur" Margin="4"/>
                                <CheckBox IsChecked="{Binding car4, Mode=OneWay}" Content="Þungaburður" Margin="4"/>
                                <CheckBox IsChecked="{Binding car5, Mode=OneWay}" Content="Rafmagnstjakkur" Margin="4"/>
                                <CheckBox IsChecked="{Binding car4, Mode=OneWay}" Content="Rafmagnströppur" Margin="4"/>
                                <CheckBox IsChecked="{Binding car5, Mode=OneWay}" Content="Startar" Margin="4"/>
                            </StackPanel>
                        </ContextMenu>
                    </Button.ContextMenu>
                    <Image Name="CarSize" Source="/sendill_client;component/Images/Truckgren32.png" MinHeight="0" Margin="2" Height="32"/>
                </Button>
                <Label Content="{Binding Path=pcarcode}" Width="50" FontSize="16" VerticalAlignment="Center" FontWeight="Bold" Padding="8,0,0,4" />
                <Button x:Name="btnCoffeebreak" CommandParameter="{Binding idcar}" Click="btnCoffeebreak_Click" Style="{StaticResource BorderlessButton}" IsEnabled="True">
                    <Image Name="InCoffeebreak" Source="/sendill_client;component/Images/CarOnPinCheck2424.png" MinHeight="0" Margin="2,2,2,8" Height="24"/>
                </Button>
                <Button x:Name="btnSendToTour" CommandParameter="{Binding idcar}" Click="btnSendToTour_Click" Style="{StaticResource BorderlessButton}" IsEnabled="True" Loaded="btnSendToTour_Loaded" >
                    <Image Name="imgSendToTour" MinHeight="0" Margin="2,2,2,2" Height="24" />
                </Button>
            </StackPanel>
            <DataTemplate.Triggers>
                <DataTrigger Binding="{Binding Path=pbreak}" Value="true">
                    <Setter TargetName="InCoffeebreak" Property="Source" Value="/sendill_client;component/Images/CarOnPinCheck2424.png" />
                    <!--CoffeeGreen_32_32.png-->
                </DataTrigger>
                <DataTrigger Binding="{Binding Path=pbreak}" Value="false">
                    <Setter TargetName="InCoffeebreak" Property="Source" Value="/sendill_client;component/Images/CarOnPinUncheck.png" />
                    
                    <!--CoffeeGreen_32_32.png-->
                </DataTrigger>
                <DataTrigger Binding="{Binding Path=carsize}" Value="1">
                    <Setter TargetName="CarSize" Property="Source" Value="/sendill_client;component/Images/TruckRed32.png" />
                    <Setter TargetName="lblIdcar" Property="Foreground" Value="#FFEF1111"></Setter>
                    <!--CoffeeGreen_32_32.png-->
                </DataTrigger>
                <DataTrigger Binding="{Binding Path=carsize}" Value="2">
                    <Setter TargetName="CarSize" Property="Source" Value="/sendill_client;component/Images/TruckYellow32.png" />
                    <Setter TargetName="lblIdcar" Property="Foreground" Value="#FFDF8C12"></Setter>
                    <!--CoffeeGreen_32_32.png-->
                </DataTrigger>
                <DataTrigger Binding="{Binding Path=carsize}" Value="3">
                    <Setter TargetName="CarSize" Property="Source" Value="/sendill_client;component/Images/Truckgren32.png" />
                    <Setter TargetName="lblIdcar" Property="Foreground" Value="#FF096107"></Setter>
                    <!--CoffeeGreen_32_32.png-->
                </DataTrigger>
                <DataTrigger Binding="{Binding Path=listed}" Value="true">
                    <Setter TargetName="imgSendToTour" Property="Source" Value="/sendill_client;component/Images\redkross32.png"/>
                    </DataTrigger>
                <DataTrigger Binding="{Binding Path=listed}" Value="false">
                    <Setter TargetName="imgSendToTour" Property="Source" Value="/sendill_client;component/Images\ArrToTour.png"/>
                </DataTrigger>
            </DataTemplate.Triggers>
        </DataTemplate>

